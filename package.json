{
  "name": "@rdolcegroup/runlintic-app",
  "version": "8.0.0",
  "description": "A toolkit for automating code quality, cleanup, and releases.",
  "keywords": [
    "release",
    "automation",
    "changelog"
  ],
  "homepage": "https://github.com/R-Dolce-Group/runlintic-app#readme",
  "bugs": {
    "url": "https://github.com/R-Dolce-Group/runlintic-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/R-Dolce-Group/runlintic-app.git"
  },
  "license": "MIT",
  "author": "R. Dolce Group",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "default": "./lib/index.js"
    }
  },
  "main": "./lib/index.js",
  "bin": {
    "runlintic": "bin/runlintic-app.js"
  },
  "files": [
    "bin",
    "lib/api",
    "lib/configs",
    "lib/core",
    "lib/dashboard/dist",
    "lib/index.js",
    "lib/scripts/postinstall.js",
    "lib/scripts/dependency-analysis.js",
    "lib/templates",
    "lib/utils",
    "docs",
    "public",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "check-all": "npm run lint && npm run typecheck",
    "clean:all": "rm -rf node_modules package-lock.json pnpm-lock.yaml",
    "commit": "node scripts/generate-commit.js",
    "decision:benchmark": "node lib/framework/benchmark-suite.js --benchmark",
    "decision:evaluate": "node lib/framework/decision-scorecard.js --evaluate",
    "decision:quick": "node lib/framework/decision-scorecard.js --quick",
    "depcheck": "depcheck || exit 0",
    "deps:analyze": "node lib/scripts/dependency-analysis.js --report=detailed",
    "deps:audit": "npm audit",
    "deps:audit-fix": "npm audit fix",
    "deps:check": "echo 'âœ… Security override packages (lilconfig, picocolors, prompts) are intentionally unused - they replace vulnerable dependencies via overrides'",
    "deps:clean": "npm run clean:all && npm install",
    "deps:dedupe": "npm dedupe",
    "deps:health": "node lib/scripts/dependency-analysis.js --comprehensive",
    "deps:lockfile-check": "npm ci --dry-run",
    "deps:outdated": "npm outdated",
    "deps:security": "node lib/scripts/dependency-analysis.js --focus=security",
    "deps:sync": "npm install --package-lock-only",
    "deps:unused": "depcheck",
    "deps:update": "npm update",
    "deps:update-latest": "npm update",
    "deps:updates": "node lib/scripts/dependency-analysis.js --focus=updates",
    "deps:validate": "npm run deps:check && npm run deps:audit && npm run deps:lockfile-check",
    "fastify:prototype": "node lib/framework/fastify-prototype.js --port 3001",
    "format": "prettier --write \"**/*.{ts,tsx,md}\" && sort-package-json package.json",
    "framework:setup": "node lib/framework/quality-gates.js --setup",
    "health-check": "npm run lint && npm run typecheck && npm run maintenance && npm run deps:validate",
    "postinstall": "node lib/scripts/postinstall.js",
    "knip": "knip",
    "lint": "eslint . --max-warnings 0 --format=json",
    "lint:fix": "eslint . --fix && sort-package-json package.json",
    "maintenance": "npm run deps:check && npm run depcheck && npm run manypkg:fix",
    "manypkg:fix": "sort-package-json package.json",
    "monitor:health": "curl http://localhost:3000/api/health",
    "monitor:performance": "node lib/framework/performance-monitoring.js",
    "prepack": "npm run lint && npm run typecheck",
    "prepare": "husky",
    "quality:gates": "node lib/framework/quality-gates.js --run-all",
    "release": "lib/scripts/runlintic-app-release-with-auth.sh",
    "release:beta": "lib/scripts/runlintic-app-release-with-auth.sh --preRelease=beta",
    "release:dry": "lib/scripts/runlintic-app-release-with-auth.sh --dry-run",
    "release:major": "lib/scripts/runlintic-app-release-with-auth.sh --increment=major",
    "release:minor": "lib/scripts/runlintic-app-release-with-auth.sh --increment=minor",
    "release:patch": "lib/scripts/runlintic-app-release-with-auth.sh --increment=patch",
    "test": "echo 'No tests configured - run npm run test:cli for CLI testing'",
    "test:cli": "node bin/runlintic-app.js help",
    "test:cli:all": "npm run test:cli && npm run test:cli:health && npm run test:cli:release:dry",
    "test:cli:health": "node bin/runlintic-app.js health-check",
    "test:cli:release:dry": "node bin/runlintic-app.js release:dry",
    "typecheck": "tsc --noEmit --skipLibCheck || echo 'No TypeScript files found - skipping typecheck'",
    "version": "conventional-changelog -p conventionalcommits -i CHANGELOG.md -s && git add CHANGELOG.md"
  },
  "overrides": {
    "@octokit/plugin-paginate-rest": "^11.5.0",
    "ansi-regex": "6.1.0",
    "ansi-styles": "6.2.1",
    "chalk": "npm:picocolors@^1.1.0",
    "cliui": "8.0.1",
    "color-convert": "2.0.1",
    "color-name": "2.0.0",
    "depcheck": {
      "cosmiconfig": "^7.1.0"
    },
    "inquirer": "npm:prompts@^2.4.2",
    "is-arrayish": "0.3.2",
    "ora": "8.1.1",
    "string-width": "7.2.0",
    "strip-ansi": "7.1.0",
    "supports-color": "9.4.0",
    "wrap-ansi": "9.0.0",
    "yargs": "17.7.2"
  },
  "dependencies": {
    "cors": "2.8.5",
    "express": "5.1.0",
    "open": "10.2.0",
    "picocolors": "1.1.1",
    "prompts": "2.4.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@fastify/compress": "8.1.0",
    "@fastify/cors": "11.1.0",
    "@fastify/rate-limit": "10.3.0",
    "@next/eslint-plugin-next": "^15.0.3",
    "@release-it/conventional-changelog": "8.0.1",
    "@typescript-eslint/eslint-plugin": "^8.33.0",
    "conventional-changelog-cli": "4.1.0",
    "conventional-changelog-conventionalcommits": "^9.1.0",
    "depcheck": "1.4.7",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-only-warn": "^1.1.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "fastify": "5.6.0",
    "globals": "^16.2.0",
    "husky": "^9.1.7",
    "knip": "^5.0.0",
    "pino": "9.11.0",
    "pino-pretty": "13.1.1",
    "prettier": "^3.6.2",
    "release-it": "17.11.0",
    "sort-package-json": "3.4.0",
    "typescript": "5.9.2",
    "typescript-eslint": "^8.33.0",
    "uuid": "13.0.0",
    "winston": "3.17.0"
  },
  "engines": {
    "node": ">=22.16.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "knip": {
    "entry": [
      "bin/runlintic-app.js",
      "lib/index.js",
      "lib/scripts/postinstall.js",
      "scripts/generate-commit.js"
    ],
    "project": [
      "bin/**/*.js",
      "lib/**/*.js",
      "scripts/**/*.js"
    ],
    "ignoreDependencies": [
      "lilconfig",
      "picocolors",
      "prompts"
    ]
  },
  "runkitExampleFilename": "lib/scripts/runkit.js"
}
