{
"$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "ignoreBinaries": [
        "lib/scripts/runlintic-release-with-auth.sh"
      ],
      "ignore": [
        "commitlint.config.cjs",
        ".husky/**",
        ".github/**"
      ],
      "entry": [
        "lib/index.js",
        "lib/configs/*.js",
        "env.mjs"
      ]
    }
  },
  "ignore": [
    "node_modules/**",
    ".next/**",
    ".turbo/**",
    "dist/**",
    "build/**",
    "coverage/**",
    "**/*.d.ts"
  ],
  "ignoreDependencies": [
    "@types/*",
    "auto-changelog",
    "conventional-changelog-conventionalcommits",
    "eslint-plugin-turbo"
  ],
  "includeEntryExports": false
}
